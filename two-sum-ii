class Solution:
    def twoSum(self, numbers: List[int], target: int) -> List[int]:

        # O(n) time O(1) space
        # 2 pointer
        
        left, right = 0, len(numbers)-1

        while left < right:
            currVal = numbers[left] + numbers[right]
            if currVal == target:
                return [left+1, right+1] 
            if currVal < target:
                left+=1
            else:
                right-=1

            
